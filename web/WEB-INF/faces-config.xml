<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Login</from-outcome>
            <to-view-id>/Login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>RegistrarProfesor</from-outcome>
            <to-view-id>/RegistraProfesor.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>RegistrarAlumno</from-outcome>
            <to-view-id>/RegistraAlumno.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Alumno</from-outcome>
            <to-view-id>/InicioAlumno.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Profesor</from-outcome>
            <to-view-id>/InicioProfesor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/InicioProfesor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Modificar</from-outcome>
            <to-view-id>/modificarProfesor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/InicioAlumno.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/RegistraProfesor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/RegistraAlumno.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/modificarProfesor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Modificar</from-outcome>
            <to-view-id>/modificarProfesor.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Exito</from-outcome>
            <to-view-id>/InicioProfesor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
